{
  "expo": {
    "name": "CloudSync",
    "slug": "cloudsync",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cloudsync.app"
    },
    "android": {
      "package": "com.cloudsync.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "cloudsync",
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow CloudSync to access your photos to upload files."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}
